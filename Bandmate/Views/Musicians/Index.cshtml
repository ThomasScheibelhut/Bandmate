@model IEnumerable<Bandmate.Models.Musicians>

@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Index</title>
</head>
<body>
<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Name)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.City)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.State)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Zip)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.CreationDate)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Description)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.ProfilePicture)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.OpenToGigs)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.OpenToRecording)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.OpenToJoiningBands)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Name)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.City)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.State)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Zip)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.CreationDate)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Description)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.ProfilePicture)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.OpenToGigs)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.OpenToRecording)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.OpenToJoiningBands)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.MusicianId">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.MusicianId">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.MusicianId">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
</body>
</html>
